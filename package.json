{
  "name": "setarea",
  "version": "1.0.0",
  "description": "Allows creating areas on a canvas and exports them to a ComfyUI workflow.",
  "main": "setarea.js",
  "scripts": {
    "watch:sass": "onchange 'src/**/*.sass' -- npm run build:css",
    "watch:tailwind": "onchange 'dist/*.css' -- npm run build:tailwind",
    "dev": "concurrently \"npm run watch:sass\" \"npm run watch:tailwind\""
  },
  "keywords": [
    "comfyui"
  ],
  "author": "_ka_de",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "concurrently": "^8.2.2",
    "css-loader": "^6.10.0",
    "eslint": "^8.57.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "node-sass": "^9.0.0",
    "onchange": "^7.1.0",
    "postcss": "^8.4.38",
    "postcss-cli": "^11.0.0",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^3.4.3",
    "watch": "^1.0.2",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4"
  }
}
